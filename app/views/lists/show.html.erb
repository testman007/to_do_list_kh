<div class = "form container">
  <div class="page-header">
    <h1>Event Datails <small>things need to done</small></h1>
  </div>

  <ul class="list-group">
    <li class="list-group-item"><strong>Name : </strong><%= @list.event %></li>
    <li class="list-group-item"><strong>Due Date : </strong><%= @list.duedate%></li>
    <li class="list-group-item"><strong>Note : </strong><%= simple_format(@list.description) %></li>
  </ul>

  <button class="btn btn-default" type="submit">
    <%= link_to 'Back to index', lists_path %>
  </button>
</div>